#include <stdint.h>

void mac_16_32_32_c_byte_@OPTIMIZATION@(int32_t *add_res, int16_t *mul1, int16_t *mul2, unsigned int len)
{
	for (int i = 0; i < len; i++)
		add_res[i] = add_res[i] + (mul1[i] * mul2[i]);
}
