#include <stdint.h>

void mac_8_16_32_c_byte_@OPTIMIZATION@(int32_t *res, int16_t *add, int8_t *mul1, int8_t *mul2, unsigned int len)
{
	for (int i = 0; i < len; i++)
		res[i] = add[i] + (mul1[i] * mul2[i]);
}
