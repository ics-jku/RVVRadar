# version
RVVBMARK_VERSION="0"
RVVBMARK_SUBVERSION="1"

# Does toolchain support RISC-V
RVVBMARK_RV_SUPPORT=@RVVBMARK_RV_SUPPORT@

# Does toolchain support RISC-V vector extension (RVV)
# (can be built for non-rvv when set to zero)
RVVBMARK_RVV_SUPPORT=@RVVBMARK_RV_SUPPORT@

# extra flags
# (mainly used to add -march + vector -> see configure)
RVVBMARK_EXTRA_ASFLAGS=@RVVBMARK_EXTRA_ASFLAGS@
RVVBMARK_EXTRA_CFLAGS=@RVVBMARK_EXTRA_CFLAGS@

# install prefix
RVVBMARK_INSTALL_PREFIX=@RVVBMARK_INSTALL_PREFIX@
